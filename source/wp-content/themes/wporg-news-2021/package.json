{
	"name": "wporg-news-2021-theme",
	"version": "1.0.0",
	"description": "A Full Site Editing theme for w.org/news, with a jazz-inspired design.",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"dependencies": {},
	"devDependencies": {
		"@wordpress/base-styles": "^3.6.0",
		"@wordpress/browserslist-config": "^4.1.0",
		"@wordpress/scripts": "^17.0.0",
		"autoprefixer": "^10.4.0",
		"chokidar-cli": "^3.0.0",
		"node-sass": "^4.12.0",
		"node-sass-package-importer": "^5.3.2",
		"postcss": "^8.4.4",
		"postcss-cli": "^9.0.2",
		"rtlcss": "^3.3.0"
	},
	"scripts": {
		"build": "node build-styles.js",
		"start": "chokidar \"sass/**/*.scss\" -c \"node build-styles.js\" --initial",
		"lint:css": "wp-scripts lint-style sass",
		"lint:js": "echo \"No JS.\""
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"rtlcssConfig": {
		"options": {
			"autoRename": false,
			"autoRenameStrict": false,
			"blacklist": {},
			"clean": true,
			"greedy": false,
			"processUrls": false,
			"stringMap": []
		},
		"plugins": [],
		"map": false
	}
}
