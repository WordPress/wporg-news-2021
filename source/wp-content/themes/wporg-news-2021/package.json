{
	"name": "wporg-news-2021-theme",
	"version": "1.0.0",
	"description": "A Full Site Editing theme for w.org/news, with a jazz-inspired design.",
	"author": "WordPress.org",
	"license": "GPL-2.0-or-later",
	"private": true,
	"dependencies": {},
	"devDependencies": {
		"@wordpress/base-styles": "^5.0.0",
		"@wordpress/browserslist-config": "^6.0.0",
		"@wordpress/scripts": "^27.9.0",
		"autoprefixer": "^10.4.7",
		"chokidar-cli": "^3.0.0",
		"postcss": "^8.4.14",
		"rtlcss": "^3.5.0",
		"sass": "^1.52.3"
	},
	"scripts": {
		"build": "node build-styles.js --no-rtl",
		"start": "chokidar \"sass/**/*.scss\" -c \"node build-styles.js --no-rtl\" --initial",
		"lint:css": "wp-scripts lint-style sass",
		"lint:js": "echo \"No JS.\""
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"stylelint": {
		"extends": "../../../../.stylelintrc",
		"rules": {
			"max-line-length": null,
			"no-duplicate-selectors": null
		}
	}
}
